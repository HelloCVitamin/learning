<?xml version="1.0" encoding="UTF-8"?>
<module name="logging">
	<InFlow>
		<handler name="InFlowHandler" class="test.simple.handler.LogHandler">
			<order phase="loggingPhase"/>
		</handler>
	</InFlow>
	<OutFlow>
		<handler name="OutFlowHandler" class="test.simple.handler.LogHandler">
			<order phase="loggingPhase"/>
		</handler>
	</OutFlow>
	
	<OutFaultFlow>
		<handler name="OutFaultFlowHandler" class="test.simple.handler.LogHandler">
			<order phase="loggingPhase"/>
		</handler>
	</OutFaultFlow>
	
	<InFaultFlow>
		<handler name="InFaultFlowHandler" class="test.simple.handler.LogHandler">
			<order phase="loggingPhase"/>
		</handler>
	</InFaultFlow>
	
</module>
